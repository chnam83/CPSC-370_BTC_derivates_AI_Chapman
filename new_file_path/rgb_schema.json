{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RGB Color",
  "type": "object",
  "properties": {
    "red": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "green": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "blue": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    }
  },
  "required": ["red", "green", "blue"]
}
